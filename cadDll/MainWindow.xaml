<Window x:Class="cadDll.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cadDll"
        mc:Ignorable="d"
        Title="MainWindow" Height="375.634" Width="134.577">
    <StackPanel HorizontalAlignment="Left">
        <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <CheckBox Content="CheckBox"/>
        <ComboBox Width="120"/>
        <Rectangle Fill="#FFF4F4F5" Height="100" Stroke="Black" Width="100"/>
        <TextBlock TextWrapping="Wrap" Text="{Binding ElementName=slider, Path=Value}"/>
        <Slider Name="slider"/>
        <RadioButton Content="RadioButton"/>
        <Button Content="Button" Width="75" Click="Button_Click"/>
        <TextBox Name="textBox1" Height="89" TextWrapping="Wrap" Width="120"/>
    </StackPanel>
</Window>
